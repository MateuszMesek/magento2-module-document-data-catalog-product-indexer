        <arguments>
            <argument name="documentName" xsi:type="const">Magento\Catalog\Model\Product::ENTITY</argument>
            <argument name="dimensionProvider" xsi:type="object">Magento\Store\Model\StoreDimensionProvider</argument>
        </arguments>
    </virtualType>
    <type name="MateuszMesek\DocumentDataCatalogProductIndexer\DataResolver">
        <arguments>
            <argument name="storeIdResolver" xsi:type="object">MateuszMesek\DocumentDataIndex\DimensionResolver\StoreIdResolver</argument>
        </arguments>
    </type>
    <type name="MateuszMesek\DocumentDataCatalogProductIndexer\EntityIdsResolver">
        <arguments>
            <argument name="storeIdResolver" xsi:type="object">MateuszMesek\DocumentDataIndex\DimensionResolver\StoreIdResolver</argument>
        </arguments>
    </type>
    <type name="MateuszMesek\DocumentDataCatalogProductIndexer\IndexNameResolver">
        <arguments>
